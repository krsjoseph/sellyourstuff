<%= simple_form_for(@listing) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%# f.input :image, as: :file %>
    <%= file_field_tag "images[]", type: :file, multiple: true %>
    <%= f.input :description %>
    <%= f.input :location %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
